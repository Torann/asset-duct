{
    "name": "torann/duct",
    "description": "Asset Duct for Laravel 4.",
    "keywords": ["laravel", "duct", "assets", "asset management", "asset pipeline", "pipeline"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "daniel@lyften.com"
        }
     ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.2",
        "illuminate/console": "~4.2",
        "oyejorge/less.php": "~1.5",
        "mrclay/minify": "~2.2.0"
    },
    "suggest": {
        "imagine/imagine": "Enable image compressors"
    },
    "autoload": {
        "psr-0": {
            "Torann\\Duct": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
